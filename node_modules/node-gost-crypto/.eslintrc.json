{
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "rules": {
    "no-extra-semi": 0,
    "semi": 0,
    "no-var": 0,
    "no-unused-vars": 0,
    "no-redeclare": 0,
    "no-empty": 0,
    "no-useless-escape": 0,
    "no-undef": 0,
    "no-const-assign": 0,
    "valid-typeof": 0,
    "no-fallthrough": 0,
    "no-prototype-builtins": 0,
    "no-case-declarations": 0
  }
}
